<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfilepageActicity">
    <RelativeLayout
        android:id="@+id/hedarview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@drawable/background_card_menu"
        android:elevation="5dp"
        >
        <View
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:background="@drawable/background_card_menu"
            />
        <ImageView
            android:id="@+id/imgbacks"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/back"
            android:elevation="7dp"
            android:paddingStart="10dp"
            android:layout_marginTop="10dp"
            tools:ignore="ContentDescription,RtlSymmetry" />
        <ImageView
            android:id="@+id/imgpilihan"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@id/imglogos"
            android:src="@drawable/pilihan"
            tools:ignore="ContentDescription" />
        <ImageView
            android:id="@+id/imglogos"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:src="@drawable/logoh"
            android:layout_marginStart="80dp"
            android:layout_marginTop="5dp"
            tools:ignore="ContentDescription" />
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:src="@drawable/akuns7"
        android:layout_marginBottom="80dp"
        tools:ignore="ContentDescription" />
   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_centerHorizontal="true"
       android:layout_marginTop="50dp"
       >
       <Button
           android:id="@+id/logouts"
           android:layout_width="200dp"
           android:layout_height="40dp"
           android:layout_below="@id/hedarview"
           android:layout_marginTop="510dp"
           android:text="Log Out      "
           android:textSize="15sp"
           android:layout_centerHorizontal="true"
           android:background="@drawable/buton_saya"
           android:fontFamily="@font/poppinssemibold"
           tools:ignore="HardcodedText,NotSibling" />
       <ImageView
           android:layout_width="30dp"
           android:layout_height="30dp"
           android:src="@drawable/logout"
           android:layout_marginTop="515dp"
           android:layout_marginStart="230dp"
           tools:ignore="ContentDescription" />
   </RelativeLayout>


    <RelativeLayout
        android:id="@+id/footerview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/background_card_menu"
        android:elevation="10dp"
        >
        <View
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:background="@drawable/background_card_menu"
            />
        <ImageView
            android:id="@+id/imgberandas"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/beranda1"
            android:elevation="7dp"
            android:layout_marginStart="30dp"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/imgkeranjangs"
            android:layout_width="50dp"
            android:layout_height="60dp"
            android:layout_marginStart="30dp"
            android:layout_toEndOf="@id/imgberandas"
            android:elevation="7dp"
            android:src="@drawable/keranjang1"
            tools:ignore="ContentDescription,ImageContrastCheck" />
        <ImageView
            android:id="@+id/imgpesans"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/pesans1"
            android:elevation="7dp"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@id/imgkeranjangs"
            android:layout_marginStart="30dp"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/imgakuns"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="30dp"
            android:layout_toEndOf="@id/imgpesans"
            android:elevation="7dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/akun2"
            tools:ignore="ContentDescription,ImageContrastCheck" />
    </RelativeLayout>

</RelativeLayout>